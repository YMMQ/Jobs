<p id="notice"><%= notice %></p>

<h1>User Infos</h1>

<table>
  <thead>
    <tr>
      <th>Fname</th>
      <th>Lname</th>
      <th>Gender</th>
      <th>Dob</th>
      <th>Picture</th>
      <th>Mobile</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_infos.each do |user_info| %>
      <tr>
        <td><%= user_info.fname %></td>
        <td><%= user_info.lname %></td>
        <td><%= user_info.gender %></td>
        <td><%= user_info.dob %></td>
        <td><%= user_info.picture %></td>
        <td><%= user_info.mobile %></td>
        <td><%= user_info.user %></td>
        <td><%= link_to 'Show', user_info %></td>
        <td><%= link_to 'Edit', edit_user_info_path(user_info) %></td>
        <td><%= link_to 'Destroy', user_info, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Info', new_user_info_path %>
