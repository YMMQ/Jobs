<p id="notice"><%= notice %></p>

<h1>Educations</h1>

<table>
  <thead>
    <tr>
      <th>Institute</th>
      <th>Degree</th>
      <th>Tmarks</th>
      <th>Omarks</th>
      <th>Sdate</th>
      <th>Endyear</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @educations.each do |education| %>
      <tr>
        <td><%= education.institute %></td>
        <td><%= education.degree %></td>
        <td><%= education.tmarks %></td>
        <td><%= education.omarks %></td>
        <td><%= education.sdate %></td>
        <td><%= education.endyear %></td>
        <td><%= education.user %></td>
        <td><%= link_to 'Show', education %></td>
        <td><%= link_to 'Edit', edit_education_path(education) %></td>
        <td><%= link_to 'Destroy', education, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Education', new_education_path %>
